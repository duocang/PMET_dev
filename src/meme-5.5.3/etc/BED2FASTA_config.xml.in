<appConfig xmlns="http://nbcr.sdsc.edu/opal/types" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <metadata appName="BED2FASTA @SERVICE_VERSION@">
    <usage>Bed2Fasta: Generate BED file from FASTA file</usage>
    <info xsd:type="xsd:string">
    <![CDATA[
    bed2fasta_webservice [options] <bed file> <db seqs> <output file>
    ]]>
    </info>
    <types>
      <untaggedParams>
        <param>
          <id>genome</id>
          <paramType>STRING</paramType>
          <textDesc>Filename of Genome FASTA</textDesc>
        </param>
      </untaggedParams>
      <groups>
      </groups>
    </types>
  </metadata>
  <binaryLocation>@SERVICE_DIR@/bed2fasta_webservice</binaryLocation>
  <drmaaQueue>@drmaa_queue_short@</drmaaQueue>
  <parallel>false</parallel>
</appConfig>
